{
  "name": "v1z3r-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    ".",
    "modules/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:modules": "npm run test --workspaces",
    "build:modules": "npm run build --workspaces",
    "setup:dev": "echo \"Setting up development environment...\"",
    "infra:dev": "cd infra/cdk && cdk deploy --all --profile dev",
    "infra:staging": "cd infra/cdk && cdk deploy --all --profile staging",
    "infra:prod": "cd infra/cdk && cdk deploy --all --profile prod",
    "infra:destroy": "cd infra/cdk && cdk destroy --all",
    "deploy:dev": "npm run build && echo \"Deploy to dev environment\"",
    "deploy:prod": "npm run build && echo \"Deploy to production environment\""
  },
  "resolutions": {
    "@tensorflow/tfjs-converter": "4.22.0",
    "@tensorflow/tfjs-core": "4.22.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.11.0",
    "@types/qrcode.react": "^3.0.0",
    "axios": "^1.8.2",
    "framer-motion": "^12.5.0",
    "ml5": "^0.12.2",
    "next": "^14.2.30",
    "npm-force-resolutions": "^0.0.10",
    "p5": "^1.7.0",
    "playwright": "^1.53.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "three": "^0.158.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/three": "^0.158.1",
    "autoprefixer": "^10.4.16",
    "aws-cdk": "^2.1019.2",
    "aws-cdk-lib": "^2.202.0",
    "constructs": "^10.4.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
