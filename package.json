{
  "name": "v1z3r-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    ".",
    "modules/*"
  ],
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:auto": "next dev",
    "dev:local": "cp .env.local .env && next dev -p 3000",
    "dev:dev": "cp .env.dev .env && next dev -p 3000",
    "prebuild": "rimraf .next",
    "build": "next build",
    "build:dev": "cp .env.dev .env && next build",
    "build:prod": "cp .env.prod .env && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:modules": "yarn workspace @vj-app/visual-renderer test && yarn workspace @vj-app/vj-controller test && yarn workspace @vj-app/sync-core test && yarn workspace @vj-app/preset-storage test && yarn workspace @vj-app/lyrics-engine test",
    "build:modules": "cd modules/visual-renderer && yarn build && cd ../vj-controller && yarn build && cd ../sync-core && yarn build && cd ../preset-storage && yarn build && cd ../lyrics-engine && yarn build && cd ../..",
    "setup:dev": "yarn install && yarn build:modules && echo \"Development environment setup complete\"",
    "infra:dev": "cd infra/cdk && cdk deploy VjUnifiedStack-dev --require-approval=never",
    "infra:prod": "cd infra/cdk && cdk deploy VjUnifiedStack-prod --require-approval=never --context stage=prod",
    "infra:destroy:dev": "cd infra/cdk && cdk destroy VjUnifiedStack-dev",
    "infra:destroy:prod": "cd infra/cdk && cdk destroy VjUnifiedStack-prod",
    "deploy:dev": "yarn build:dev && echo \"Deploy to dev environment\"",
    "deploy:prod": "yarn build:prod && echo \"Deploy to production environment\"",
    "env:local": "cp .env.local .env",
    "env:dev": "cp .env.dev .env",
    "env:prod": "cp .env.prod .env",
    "prepare": "husky install"
  },
  "resolutions": {
    "@tensorflow/tfjs-converter": "4.22.0",
    "@tensorflow/tfjs-core": "4.22.0",
    "axios": "^1.8.2",
    "d3-color": "^3.1.0",
    "node-fetch": "^2.6.13",
    "vega": "^5.23.0",
    "vega-functions": "^5.13.1"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@tensorflow/tfjs": "^4.11.0",
    "@types/qrcode.react": "^3.0.0",
    "axios": "^1.8.2",
    "framer-motion": "^12.5.0",
    "next": "^14.2.30",
    "npm-force-resolutions": "^0.0.10",
    "p5": "^1.7.0",
    "playwright": "^1.53.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "seedrandom": "^3.0.5",
    "three": "^0.158.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/three": "^0.158.1",
    "autoprefixer": "^10.4.16",
    "aws-cdk": "^2.1019.2",
    "aws-cdk-lib": "^2.202.0",
    "constructs": "^10.4.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "husky": "^8.0.3",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.4.31",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
